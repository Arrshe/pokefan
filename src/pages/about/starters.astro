---
import { getCollection } from "astro:content";
import MarkdownPostLayout from "../../layouts/MarkdownPostLayout.astro";

const about = await getCollection("about");

const starters = about.find((aboutPage: { slug: string; }) => {
  return aboutPage.slug === "starters";
}); // Accede al contenido de la pÃ¡gina de starters

const { Content } = await starters!.render();
---

<MarkdownPostLayout frontmatter={starters?.data}>
  <Content />
</MarkdownPostLayout>